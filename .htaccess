DirectoryIndex index.php
Options -Indexes
RewriteEngine on
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}.php -f
RewriteRule ^(.*)$ $1.php
Header set Cache-Control "max-age=2628000, public"
ErrorDocument 403 /soxbty.com/error.php
ErrorDocument 404 /soxbty.com/error.php
ErrorDocument 500 /soxbty.com/error.php